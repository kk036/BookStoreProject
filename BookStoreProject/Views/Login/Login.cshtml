
@{
    ViewBag.Title = "Login";
}


<!-- about -->
<section class="about py-5">
    <div class="container py-md-5">
          <div class="row">

            <div class="col-lg-12 contact-right-wthree-info login">
                <h5 class="text-center mb-12">Admin Login </h5>
                <form action="chkLogin" method="post">
                    <div class="form-group mt-12">
                        <label>Eamil</label>
                        <input id="txtEmail" type="email" name="txtEmail" class="form-control" placeholder="" required="" />
                    </div>

                    <div class="form-group mt-12">
                        <label class="mb-12">Password</label>
                        <input id="txtPassword" type="password" name="txtPassword" class="form-control" placeholder="" required="" />
                    </div>
                    <input id="btnSubmit" class="btn btn-primary submit mb-12" type="submit" value="Sign In " />


                </form>

            </div>
        </div>

    </div>
</section>
